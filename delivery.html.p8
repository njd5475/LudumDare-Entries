version 41
__lua__

local objs,ctx={}, {score=0, cycle={}}

function _init()
 add(objs, sheepspwnr())
 add(objs, pillow(16, 16))
 
 for i=1,4 do
 	local c = {
 		num=flr(rnd(100)),
 		mark=false
 	}
 	ctx.cycle[c.num]=c
 end
end

function _draw()
 cls(3)
 for y=1,128,16 do
 	line(0, y, 128, y, 11)
 end
 for o in all(objs) do
  if o.draw then
	 	o:draw()
 	end
 end
 
 local cx,cy=4,100
 for _, c in pairs(ctx.cycle) do
  local _c = c.mark and 5 or 14
 	print(c.num, cx, cy, _c)
 	cx += #tostr(c.num)*8 + 8
 end
 local scr = ctx.score .. '000'
 print(scr, 128-#scr*5, 4, 9)
 
 print('⬆️ up  ⬇️ down', 4, 120, 10)
 if btn(2) then
 	print('⬆️', 4, 120, 7)
 end
 
 if btn(3) then
  print('⬇️', 6*5+2, 120, 7)
 end
end

function _update60()
	for o in all(objs) do
	 o:update(ctx)
	end
end

