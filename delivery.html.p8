-- pillow

function pillow(x,y)
	return {
	 _type='pillow',
		draw=function()
			spr(18, x,y)
		end,
		update=function(me, ctx)
			if btnp(2) then
				y -= 16
			end
			if btnp(3) then
				y += 16
			end
			
			y = min(16*5,max(0,y))
			for o in all(objs) do
				-- check collisions
				if o._type =='sheep' and o:hit(x,y) then
					del(objs, o)

					ctx.score += o.reward
					if o.num > -1 and ctx.cycle[o.num] then
						ctx.cycle[o.num].mark = true
						sfx(62) -- good
					elseif o.num > -1 and not ctx.cycle[o.num] then
						sfx(63) -- bad
					else
						sfx(0)
					end
				end
			end
		end
	}
end

