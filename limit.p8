local dbgmsg='debugmsg'
local dbgt = 30

local state
local gravfn=nil

function _init()
	state=main()
	state:addfx(temp)
	state:addfx(obstruct)
	state:push(ctrl())
	
	state:place(1,8,64,2,2)
 state:place(1,104,64,2,2)
 
 state:push(player(6,12,62,false))
 state:push(player(7,108,64,true))
 local mn,mx=38,88
 for i=1,5 do
	 state:place(5,rnd(mx-mn)+mn,rnd(mx-mn)+mn,1,1)
 end
end

function _draw()
	state:draw()
	
	if dbgt > 0 and dbgmsg and #dbgmsg > 0 then
		print(dbgmsg, 4, 4, 12)
	end
	
	print(stat(7), 118, 4, 12)
end

function _update60()
	state = state:update()
			
	if dbgt > 0 and dbgmsg and #dbgmsg > 0 then
		dbgt -= 1
		if dbgt <= 0 then
			dbgmsg = nil
		end
	end

 if dbgt <= 0 and dbgmsg and #dbgmsg > 0 then
 	dbgt = 60
 end
	
end
