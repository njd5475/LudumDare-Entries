-- options

function mkopt(sp,fn,after)
	return function(c)
		return {
			draw=function(_,x,y)
				spr(sp, x, y)
				if c.sel == _ then
					rect(x,y,x+8,y+8,7)
				end
			end,
			exec=function(_,g)
			 fn(g,c)
			end,
			after=after and after(c) or nil
		}
	end
end

-- push
push=mkopt(54, function(g,c)
end)

-- heat spell
heat=mkopt(53, function(g,c)
	g:push(heater(c.x,c.y,300))
end,push)

-- gravity well spell
gravitywell=mkopt(52, function(g,c)
	g:push(well(c.x+4, c.y+4))
end,heat)

