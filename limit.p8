-- ctrl + options

function ctrl(p, pbtn)
	pbtn-=1
	return {
	 sel=nil,
	 x=64,
	 y=64,
	 player=p,
		draw=function(_, g)
			_.sel = _.sel or (platform(_))
			_.first = _.first or _.sel
		
		 pal(3, _.player.clr)
			spr(3, _.x, _.y)
			pal()
			
			local sx,sy=16+(1-pbtn)*64,119
			local cur = _.first
			while cur and cur ~= fwd do
			 cur:draw(sx,sy)
			 sx += 9
				cur = cur.after
   end
		end,
		update=function(_, g)
		 local dx,dy=0,0
			for i=1,4 do
				if btn(i-1,pbtn) then
					dx=adjx[i]
					dy=adjy[i]
				end
			end
			_.x += dx
			_.y += dy
			
			if btnp(4,pbtn) then
				_:exec(g)
			end
			
		 if btnp(5,pbtn) then
				_:selnext()
			end

		end,
		exec=function(_, g)
			_.sel:exec(g)
		end,
		selnext=function(_)
			_.sel = _.sel.after
			if not _.sel then
		 	_.sel=_.first
		 end
		end
	}
end

-- options

function mkopt(sp,fn,after)
	return function(c)
		return {
			draw=function(_,x,y)
			 spr(sp, x, y)
				if c.sel == _ then
					rect(x,y,x+8,y+8,7)
				end
			end,
			exec=function(_,g)
			 sfx(0)
			 fn(g,c)
			end,
			after=after and after(c) or nil
		}
	end
end

