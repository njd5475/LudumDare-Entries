-- fxs

function playerreactor(_p)
	return function(c)
		local d2 = distsq(c.x, c.y, _p.x+4, _p.y+4)
	 
	 if d2 < 64 and (abs(c.dx) > 0.5 or abs(c.dy) > 0.5) then
	 	c.dx -= c.dx * 0.25
	 	c.dy -= c.dy * 0.25
	 	local dmg=(1+(c:tmpperc() > 0.25 and rnd(5) or 0))
	 	_p.health-=dmg
	 	dbgmsg=_p.name .. ' player hurt ' .. dmg .. ' dmg'
	 end
	end
end

function pforce(_x,_y,dx,dy,amt)
	
	return function(c)
	 local d2 = distsq(c.x, c.y, _x, _y)
	 
	 if d2 < 150 then
		 c.dx += dx
		 c.dy += dy
		end
	end
end

function temp(c)
	if c.tmp > c.ris then
		c.dx+=(rnd()*0.05-0.025)*(c.tmp/c.tmpmx)/16
	 c.dy+=(rnd()*0.05-0.025)*(c.tmp/c.tmpmx)/16
		c.tmp -= 1 -- cool
	end
	c.x += c.dx
	c.y += c.dy
end

function heatarea(hx, hy, a)
	return function(c)
		local d2 = distsq(c.x, c.y, hx, hy)
	
		if d2 <= a then
			c.tmp += rnd(c.tmpmx-c.tmp)
		end
	end
end

local gravg = 9.8
function grav(gx,gy,wgt)
 wgt = wgt or 150

	return function(c)
		local d2 = distsq(c.x, c.y, gx, gy)
		
		if d2 > 25 and d2 < 300 then
			local dirx, diry = vecdir(gx, gy, c.x, c.y)
			
		 	dirx,diry=vecnorm(dirx,diry)
			
			local magg = gravg * ((wgt * c.wgt) / d2)
			if magg > 0.05 then
				c.x += dirx * min(magg, 10)
				c.y += diry * min(magg, 10)
			end
		end
	end
end

function obstruct(c, g)
	if abs(c.dx) >= 1 or abs(c.dy) >= 1 then
		return
	end 
 

	local obs = g:get(c.x, c.y)
	if obs and obs ~= c then
		local adj = g:getadj(c.x, c.y)
		-- 1=left,2=right,3=top,4=bot,
		-- 5=left-top,6=right-top,7=bottom-right,8=bottom-left
		for i=1,#adj do
			if adj[i] == nil then
			 -- find the first and move to it
				c.x+= adjx[i]
				c.y+= adjy[i]
			end
		end
		return 'revert'
	end
end
