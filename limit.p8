-- ctrl

function ctrl()

	return {
	 sel=nil,
	 x=64,
	 y=64,
		draw=function(_, g)
			_.sel = _.sel or (platform(_))
			_.first = _.first or _.sel
		
			spr(3, _.x, _.y)
			
			local sx,sy=32,119
			local cur = _.first
			while cur and cur ~= fwd do
			 cur:draw(sx,sy)
			 sx += 9
				cur = cur.after
   end
		end,
		update=function(_, g)
		 local dx,dy=0,0
			for i=1,4 do
				if btn(i-1) then
					dx=adjx[i]
					dy=adjy[i]
				end
			end
			_.x += dx
			_.y += dy
			
			if btnp(4) then
				_:exec(g)
			end
			
		 if btnp(5) then
				_:selnext()
			end

		end,
		exec=function(_, g)
			_.sel:exec(g)
		end,
		selnext=function(_)
			_.sel = _.sel.after
			if not _.sel then
		 	_.sel=_.first
		 end
		end
	}
end
