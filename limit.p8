-- main

adjx={-1,1,0,0,-1,1,1,-1}
adjy={0,0,-1,1,-1,-1,1,1}

function main()
 local spwn=5
 local t = spwn 
	return {
	 objs={},
	 fxs={},
	 adjcback={},
	 adjc={},
	 draw=function(_)
	  cls()
	  
	  fillp(▒)
	  --rectfill(0,0,128,72,2)
	  fillp(⌂)
	  --rectfill(0,72,128,128,1)
	  fillp()
	  
	  for o in all(_.objs) do
	  	o:draw(_)
	  end
	  
	  print('cnt '.. #_.objs, 72, 4, 13)
	 end,
		update=function(_)
		 --_.adjc = _.adjcback -- swap bufs
		 --_.adjcback = {}
		 for o in all(_.objs) do
		 	o:update(_)
		 end
		 
		 t -= 1
		 if t <= 0 then
		 	--_:push(cell(rnd(64)+32,rnd(64)+32))
		 	t = spwn
		 end

			return _
		end,
		addfx=function(_, fx)
			add(_.fxs, fx)
		end,
		rmfx=function(_, fx)
			del(_.fxs, fx)
		end,
		push=function(_, o)
		 add(_.objs, o)
		end,
		excite=function(_, amt)
			for o in all(_.objs) do
				o:incex(amt or 0.01)
			end
		end,
		adjust=function(_, c)
		 local rev = false
		 local lx,ly = c.x, c.y
			_.adjc[ceil(c.x)+ceil(c.y)*128]=nil
			for fx in all(_.fxs) do
			  local r = fx(c, _)
			  rev = rev or r == 'revert'
			end
			if rev then
				c.x = lx
				c.y = ly
			end
			
			-- index
			_.adjc[ceil(c.x)+ceil(c.y)*128]=c
		end,
		get=function(_, x, y)
		 return _.adjc[ceil(x)+ceil(y)*128]
		end,
		getadj=function(_, x, y)
		 local adj = {}
			for i = 1,#adjx do
			 adj[i] = _:get(x+adjx[i], y+adjy[i])
			end
			return adj
		end,
		place=function(_,sp,x,y,w,h)
		 local spy=flr(sp/16)
		 local ox,oy=8*(sp-8*spy),spy*8
		 
			for j=1,w*8 do
			 for k=1,h*8 do
			 	local c = sget(ox+j-1,oy+k-1)
			 	if c > 0 then
			 	 local n = cell(x+j,y+k, c)
			 	 n.tmp = 0
			 		_:push(n)
			 	end
			 end
			end
		end
	}
end
