-- cell. objects
local palx,paly=0,9
local tmpmx=1000.0
local wnd={x=0,y=0,w=128,h=128}
palsorted={0,5,6,7,15,14,8,2,4,9,10,11,3,12,13,1}


function cell(_x,_y,_c)
 local dx, dy = 0, 0

	return {
	 x=_x,
	 y=_y,
	 dx=0,
	 dy=0,
	 clr=_c,
	 wgt=1, -- weight
	 tmp=rnd(tmpmx), -- temperature
	 tmpmx=tmpmx, -- temp max
	 ris=100, -- resistance
		draw=function(_,g)
			local tclr = _:tocolor()
			_.clr = _.clr or tclr
			
			local clr = _.clr
			if _:tmpperc() >= 0.5 then
				clr = sget(15*8+1, _.clr)
			end
			
		 pset(_.x,_.y,clr)
		end,
		update=function(_,g)
   g:adjust(_)
   if _.x < wnd.x or _.x > wnd.x+wnd.w or
   		_.y < wnd.y or _.y > wnd.y+wnd.h then
   	del(g.objs, _)
 		end
		end,
		incex=function(_,inc)
			_.tmp+=inc
		end,
		tmpperc=function(_)
			return _.tmp/_.tmpmx
		end,
		tocolor=function(_)
		 local perc=_.tmp/_.tmpmx
			return sget(palx+flr(perc * 8), paly)
		end
	}
end

function well(x,y)
 local isplaced,t,gfn=false,100,grav(x,y,300)
 
	return {
		draw=function()
		end,
		update=function(_,g)
		 if not isplaced then
		 	g:addfx(gfn)
		 	isplaced = true
		 end
		 
		 t -= 1
		 if t < 0 then
		  --dbgmsg='gfn removed'
		 	g:rmfx(gfn)
		 end
		end
	}
end

function heater(x,y)
 local isplaced,t,hfn=false,2,heatarea(x,y,300)
 
	return {
		draw=function()
		end,
		update=function(_,g)
		 if not isplaced then
		 	g:addfx(hfn)
		 	isplaced = true
		 end
		 
		 t -= 1
		 if t < 0 then
		  --dbgmsg='hfn removed'
		 	g:rmfx(hfn)
		 end
		end
	}
end

function player(sp, x, y, lside)
	return {
		draw=function(_)
			spr(sp, x, y, 1, 1, lside)
		end,
		update=function(_, g)
			local below=g:get(x,y)
			if below ~= nil then
				--dbgmsg='something is below me'
			else
				y += 1
				if y > 128 then
					del(g.objs, _)
					dbgmsg='player died'
				end
			end
		end
	}
end
